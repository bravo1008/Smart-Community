<!--pages/personalCenter/personalCenter.wxml-->
<view class="container">
  <view class="profile">
    <image class="avatar" src="{{avatar}}" mode="aspectFill"></image>
    <view class="info">
      <view class="item">
        <text class="title">昵称:</text>
        <text>{{nickname}}</text>
      </view>
      <view class="item">
        <text class="title">电话号:</text>
        <text>{{phone}}</text>
      </view>
      <view class="item">
        <text class="title">社区:</text>
        <text>{{community}}</text>
      </view>
    </view>
  </view>
  <button bindtap="editProfile">编辑</button>
  <view class="footer">
    <navigator url="../warranty/warranty" class="footer-item">首页</navigator>
    <navigator url="../personalCenter/personalCenter" class="footer-item">我的</navigator>
  </view>
</view>>